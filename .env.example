# EventXX Environment Variables

# Wallet Connect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Smart Contract Address on Avalanche Fuji Testnet
NEXT_PUBLIC_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890

# API Keys (if needed for external services)
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key_here
NEXT_PUBLIC_INFURA_API_KEY=your_infura_api_key_here

# Application Settings
NEXT_PUBLIC_APP_NAME=EventXX
NEXT_PUBLIC_APP_DESCRIPTION=Decentralized Event Ticketing Platform

# Development Settings
NEXT_PUBLIC_ENVIRONMENT=development
NEXT_PUBLIC_DEBUG=true